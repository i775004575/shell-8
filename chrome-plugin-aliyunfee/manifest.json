{
    "manifest_version": 2,
    "name": "Aliyun Chrome Plugin",
    "description": "ACP",
    "version": "1.0",
    "background": {
        "page": "bg.html",
        "persistent": true
    },
    "icons": {
        "16": "favicon.ico"
    },
    "permissions": [
        "https://*.aliyun.com/"
    ],
    "browser_action": {
        "default_title": "Aliyun Chrome Plugin",
        "default_icon": "favicon.ico"
    }
}